{
    "name": "scci/security-employee-tracker",
    "description": "Security Employee Tracker",
    "keywords": ["SET", "laravel", "SCCI", "DOD", "training", "security"],
    "license": "MIT",
    "homepage": "https://github.com/scci/security-employee-tracker",
    "authors": [
        {
            "name": "Shawn Dibble",
            "email": "shawndibble@gmail.com"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.8.*",
        "laravelcollective/html": "5.8.*",
        "adldap2/adldap2-laravel": "5.1.*",
        "maatwebsite/excel": "3.1.*",
        "edvinaskrucas/notification": "5.*",
        "predis/predis": "~1.0",
        "doctrine/dbal": "^2.5",
        "pusher/pusher-php-server": "^3.0",
        "rachidlaasri/laravel-installer": "~4.0",
        "graham-campbell/markdown": "^10.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "spatie/laravel-activitylog": "^3.0",
        "laravel/nexmo-notification-channel": "^2.0"
    },
    "require-dev": {
	    "symfony/dom-crawler": "~4.0",
        "symfony/css-selector": "~4.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "1.2.*",
        "phpunit/phpunit": "7.5.*",
        "laracasts/generators": "dev-master",
        "laravel/dusk": "^4.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "SET\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
	"psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {		
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 0
    }
}
