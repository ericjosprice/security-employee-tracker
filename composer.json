{
    "name": "scci/security-employee-tracker",
    "description": "Security Employee Tracker",
    "keywords": ["SET", "laravel", "SCCI", "DOD", "training", "security"],
    "license": "MIT",
    "homepage": "https://github.com/scci/security-employee-tracker",
    "authors": [
        {
            "name": "Shawn Dibble",
            "email": "shawndibble@gmail.com"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.5.*",
        "laravelcollective/html": "5.5.*",
        "adldap2/adldap2-laravel": "2.1.*",
        "maatwebsite/excel": "~2.0",
        "edvinaskrucas/notification": "5.*",
        "predis/predis": "~1.0",
        "doctrine/dbal": "^2.5",
        "pusher/pusher-php-server": "^3.0",
        "rachidlaasri/laravel-installer": "~3.0",
        "graham-campbell/markdown": "^8.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "spatie/laravel-activitylog": "^2.1"
    },
    "require-dev": {
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "1.0.*",
        "phpunit/phpunit": "6.4.*",
        "laracasts/generators": "dev-master"
    },	
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "SET\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
	"psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
		"pre-install-cmd": [
			"php -r \"unlink('bootstrap/cache/services.php');\"",
			"php -r \"unlink('bootstrap/cache/config.php');\""
		],
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "process-timeout": 0
    }
}
